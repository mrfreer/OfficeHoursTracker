<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.officehourstracker">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.android.officehourstracker.ApplicationController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ClassList"
            android:label="@string/title_activity_class_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewStudentVisitors"
            android:label="@string/title_activity_view_student_visitors"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UploadRoster"
            android:label="@string/title_activity_upload_roster"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>